{
    "name": "badrshs/scribe-ai",
    "description": "A pluggable Laravel package for AI-powered content processing and multi-channel publishing (Blogger, WordPress, Facebook, Telegram, and more).",
    "keywords": [
        "laravel",
        "publishing",
        "ai",
        "blogger",
        "wordpress",
        "content",
        "pipeline"
    ],
    "homepage": "https://github.com/badrshs/scribe-ai",
    "license": "MIT",
    "type": "library",
    "support": {
        "issues": "https://github.com/badrshs/scribe-ai/issues",
        "source": "https://github.com/badrshs/scribe-ai"
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/pipeline": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Badr\\ScribeAi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Badr\\ScribeAi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Badr\\ScribeAi\\ScribeAiServiceProvider"
            ],
            "aliases": {
                "Publisher": "Badr\\ScribeAi\\Facades\\Publisher",
                "ContentPipeline": "Badr\\ScribeAi\\Facades\\ContentPipeline"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.9",
        "phpunit/phpunit": "^11.0|^12.5",
        "binarytorch/larecipe": "^2.8"
    }
}
